{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": "warn",
    "quotes": ["error", "single"],
    "no-plusplus": "off",
    "no-underscore-dangle": "off"
  }
}
